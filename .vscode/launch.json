{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch sfssh",
      "type": "lldb",
      "request": "attach",
      "program": "${workspaceFolder}/bin/sfssh",
      "args": ["./data/image.5", "5"],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": true,
      "sourceMap": {},
      "linux": {
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb"
      },
      "osx": {
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb"
        // "MIMode": "lldb",
        // "miDebuggerPath": ""
      }
    },
    {
      "name": "test python",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/test.py",
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": true,
      "sourceMap": {}
    }
  ]
}
